{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "typedef": [true, "call-signature"],
        "no-console": false,
        "no-floating-promises": true,
        "variable-name": {
            "options": [
              "allow-leading-underscore"
            ]
          }
      },
    "rulesDirectory": []
}